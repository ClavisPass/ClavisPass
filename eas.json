{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": { "APP_VARIANT": "development" },
      "android": { "buildType": "apk" }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "channel": "production",
      "env": { "APP_VARIANT": "production" },
      "android": { "buildType": "apk" }
    }
  }
}
